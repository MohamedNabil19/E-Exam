@model IEnumerable<Exam.Models.ExamQuestion>
@{
    ViewBag.Title = "ModelExam";
    Layout = "~/Views/Shared/_Layout_student.cshtml";
}

<h2>ModelExam</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.E_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.duration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject.name)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.E_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.duration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject.name)
            </td>


            <td>
                @Html.ActionLink("Start Exam", "PreExam", new { id = item.E_id }) 
               </td>
        </tr>
    }

</table>